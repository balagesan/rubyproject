<h1>User</h1>
				<h3>Login</h3>

<%= form_tag(user_loginpage_path) do %>  
 <%= label_tag(:username) %>
 <%= text_field_tag :username, params[:username], placeholder: "Username" %><br>
 <%= label_tag(:password) %>
 <%= password_field_tag :password, params[:password], placeholder: "Password" %><br>
  <%= submit_tag("Login") %>  
  <%= link_to 'Signup', user_signup_path %>
<% end %>